<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane 
  id="UserList" 
  xmlns:fx="http://javafx.com/fxml"
  fx:controller="demo.controller.UserAddEdit">

  <children>
    <Hyperlink 
      fx:id="back" 
      text="back"
      onAction="#handleBackAction" 
      layoutY="24.0"  
      AnchorPane.rightAnchor="52.0"  />
        
    <Label 
      fx:id="message" 
      layoutX="56.0" 
      layoutY="77.0" 
      text="Please review your profile data." />
        
    <Label 
      layoutX="56.0" 
      layoutY="123.0" 
      text="Email:" />
        
    <TextField 
      fx:id="email" 
      editable="false" 
      layoutX="149.0" 
      layoutY="120.0" 
      maxWidth="2.0" 
      minHeight="30.0" 
      minWidth="215.0" 
      prefHeight="30.0" 
      prefWidth="215.0" />
        
    <Label 
      layoutX="56.0" 
      layoutY="173.0" 
      text="Phone:" />
        
    <TextField 
      fx:id="phone" 
      layoutX="149.0" 
      layoutY="171.0" 
      maxWidth="2.0" 
      minHeight="30.0" 
      minWidth="215.0" 
      prefHeight="30.0" 
      prefWidth="215.0" />
        
    <Label 
      layoutX="56.0"
      layoutY="224.0" 
      text="Address:" />
        
    <TextField 
      fx:id="address" 
      layoutX="149.0" 
      layoutY="224.0"
      maxWidth="2.0" 
      minHeight="30.0" 
      minWidth="215.0" 
      prefHeight="30.0" 
      prefWidth="215.0" />

    <Button 
      fx:id="delete" 
      text="Delete" 
      onAction="#handleDeleteAction"  
      maxHeight="2.0" 
      AnchorPane.bottomAnchor="15.0" 
      AnchorPane.rightAnchor="168.0" />
        
    <Button 
      fx:id="save"  
      text="Save" 
      onAction="#handleSaveAction" 
      defaultButton="true" 
      maxHeight="2.0" 
      maxWidth="100.0" 
      minWidth="100.0" 
      prefWidth="100.0" 
      AnchorPane.bottomAnchor="15.0" 
      AnchorPane.rightAnchor="52.0" />
    
  </children>
</AnchorPane>


